    "version": "1.0.0", 
    "license": "MIT",
    "contact": "chahuadev@gmail.com"
  },
  "helpText": {
    "header": "Chahuadev Sentinel - Universal Grammar System & Code Reader",
    "usage": "Usage: chahuadev-sentinel [options] <files...>",
    "options": [
      "--help, -h          Show this help message",
      "--version, -v       Show version number", 
      "--quiet, -q         Show only errors and summary",
      "--verbose           Show detailed output",
      "--json              Output results in JSON format",
      "--config <file>     Use custom configuration file"
    ],
    "examples": [
      "chahuadev-sentinel src/                    # Parse src directory",
      "chahuadev-sentinel **/*.js                 # Parse all JS files", 
      "chahuadev-sentinel --json src/ > report.json    # JSON output",
      "chahuadev-sentinel --quiet **/*.ts         # Quiet mode"
    ],
    "footer": "For more information, visit: https://github.com/chahuadev/chahuadev-vscode-extension"
  }
