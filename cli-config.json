  "defaultPatterns": {
    "include": "**/*.{js,ts,jsx,tsx,py,java,go,rb,php,rs,swift,kt,c,cpp,cs}",
    "exclude": "**/node_modules/**"
  },
  "fileExtensions": [".js", ".ts", ".jsx", ".tsx", ".py", ".java", ".go", ".rb", ".php", ".rs", ".swift", ".kt", ".c", ".cpp", ".cs"],
  "ignoreDirectories": ["node_modules", ".git", ".vscode", "dist", "build", "coverage", "logs", ".backups"],
  "fuzzySearch": {
    "maxLevenshteinDistance": 2,
    "maxDistance": 3,
    "minSimilarity": 0.7,
    "maxSuggestions": 3
  },
  "fallbackValues": {
    "unknownTokenName": "UNKNOWN_TOKEN",
    "parserLearningRuleId": "PARSER_LEARNING_NEEDED"
  },
